{
  "name": "vue-snakke",
  "version": "0.0.7",
  "description": "Vue 3 utility that enables you to track page scrolling progress",
  "keywords": [
    "vue",
    "vue3"
  ],
  "homepage": "https://github.com/viniciuskneves/vue-snakke#readme",
  "bugs": "https://github.com/viniciuskneves/vue-snakke/issues",
  "license": "MIT",
  "author": "Vinicius Kiatkoski Neves <viniciuskneves@icloud.com>",
  "repository": "viniciuskneves/vue-snakke",
  "files": [
    "dist",
    "types"
  ],
  "main": "./dist/vue-snakke.umd.js",
  "module": "./dist/vue-snakke.es.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "start": "vite",
    "build": "BUILD_LIB=false vite build",
    "build:lib": "vite build",
    "serve": "vite preview",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "3.1.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "1.3.0",
    "@vue/compiler-sfc": "3.1.5",
    "gitmoji-cli": "4.2.0",
    "husky": "7.0.1",
    "prettier": "2.3.2",
    "pretty-quick": "3.1.1",
    "typescript": "4.4.2",
    "vite": "2.4.4"
  }
}
